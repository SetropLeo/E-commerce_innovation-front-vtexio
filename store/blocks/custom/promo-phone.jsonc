    }
  },
  "rich-text#solution5": {
    "props": {
      "text": "PRODUÇÃO DE CONTEÚDO",
      "textPosition": "CENTER",
      "textAlignment": "CENTER"
    }
  },

  "image#about-right-phone": {
    "props": {
      "src": "https://images.squarespace-cdn.com/content/v1/5cf68a202bff5400011e5737/1561587420373-LWFIBVEIFLNJ7LOBGFBU/contact-tonya-dalton-website-book-speaking.jpg?format=2500w",
      "width": "100%",
      "height": 250
    }
  }
}
